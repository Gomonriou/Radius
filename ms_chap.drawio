        <mxCell id="lewnSp1ngnj3ixJYrkcS-2" value="&lt;span style=&quot;color: rgb(34 , 34 , 34) ; font-family: &amp;#34;helvetica&amp;#34; , &amp;#34;arial&amp;#34; , sans-serif ; font-size: 15px ; background-color: rgb(255 , 255 , 255)&quot;&gt;1. Demande une épreuve au serveur&lt;/span&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" parent="1" vertex="1">
